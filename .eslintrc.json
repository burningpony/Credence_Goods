{
  "extends": "airbnb",
  "env": {
    "node": false,
    "jest": true,
    "browser": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 10,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "es6": true,
      "forOf": true,
      "experimentalObjectRestSpread": true
    }
  },
  "plugins": ["react", "prettier"]
}
